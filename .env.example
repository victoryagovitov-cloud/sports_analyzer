# Пример файла конфигурации
# Скопируйте в .env и заполните своими значениями

# Telegram Bot API
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Настройки анализа
FUZZY_MATCH_THRESHOLD=70
FAVORITE_PROBABILITY_THRESHOLD=80
CYCLE_INTERVAL_MINUTES=50

# Настройки гандбола
HANDBALL_GOAL_DIFFERENCE=5
HANDBALL_ANALYSIS_MINUTE_START=10
HANDBALL_ANALYSIS_MINUTE_END=45
HANDBALL_TOTAL_MARGIN=4

# Настройки браузера
BROWSER_HEADLESS=True
BROWSER_TIMEOUT=30